{
  "name": "prostoycss",
  "version": "0.1.0",
  "description": "ProstoyCSS is a simple CSS Framework",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "build-task:scss-compile": "sass scss:css --style=compressed --no-source-map --quiet",
    "scss:build": "npm-run-all -p build-task:*",
    "scss:watch": "nodemon -e scss --watch scss --exec npm run scss:build",
    "dev": "npm-run-all -p scss:*"
  },

  "repository": {
    "type": "git",
    "url": "l"
  },
  "author": "Vadim Zifra",
  "license": "MIT License"
}
